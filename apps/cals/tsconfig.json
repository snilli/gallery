{
    "extends": "tsconfig/nextjs.json",
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
    ],
    "compilerOptions": {
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "exclude": [
        "src/node_modules"
    ]
}